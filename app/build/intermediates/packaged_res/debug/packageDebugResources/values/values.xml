<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="ic_launcher_background">#FDFDFD</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">JbRatingBar</string>
    <style name="Theme.JbRatingBar" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
    </style>
    <declare-styleable name="AmharicButtonView">
        <attr format="enum" name="amharic_font_type">
            <enum name="AMHARIC_FONT_NYALA" value="0"/>
            <enum name="AMHARIC_FONT_JIRET" value="1"/>
            <enum name="AMHARIC_FONT_FANTUWUA" value="2"/>
            <enum name="AMHARIC_FONT_GOFFER" value="3"/>
            <enum name="AMHARIC_FONT_HIWUA" value="4"/>
            <enum name="AMHARIC_FONT_SABAEAN" value="5"/>
            <enum name="AMHARIC_FONT_TINT" value="6"/>
            <enum name="AMHARIC_FONT_WASHRAB" value="7"/>
            <enum name="AMHARIC_FONT_WASHRASB" value="8"/>
            <enum name="AMHARIC_FONT_WOOKIANOS" value="9"/>
            <enum name="AMHARIC_FONT_YEBSE" value="10"/>
            <enum name="AMHARIC_FONT_YIGEZUBISRAT" value="11"/>
            <enum name="AMHARIC_FONT_ZELAN" value="12"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="BaseRatingBar">
        <attr format="integer" name="srb_numStars"/>
        <attr format="float" name="srb_minimumStars"/>
        <attr format="float" name="srb_rating"/>
        <attr format="dimension" name="srb_starPadding"/>
        <attr format="reference" name="srb_drawableEmpty"/>
        <attr format="reference" name="srb_drawableFilled"/>
        <attr format="boolean" name="srb_isIndicator"/>
        <attr format="boolean" name="srb_scrollable"/>
        <attr format="boolean" name="srb_clickable"/>
        <attr format="boolean" name="srb_clearRatingEnabled"/>
        <attr format="dimension" name="srb_starWidth"/>
        <attr format="dimension" name="srb_starHeight"/>
        <attr format="float" name="srb_stepSize"/>
    </declare-styleable>
    <declare-styleable name="ElevationImageView">
        <attr format="dimension" name="compatEvelation"/>
        <attr format="boolean" name="clipShadow"/>
        <attr format="boolean" name="isTranslucent"/>
        <attr format="boolean" name="forceClip"/>
    </declare-styleable>
    <declare-styleable name="RippleView">
        <attr format="integer" name="rv_alpha"/>
        <attr format="integer" name="rv_framerate"/>
        <attr format="integer" name="rv_rippleDuration"/>
        <attr format="integer" name="rv_zoomDuration"/>
        <attr format="color" name="rv_color"/>
        <attr format="boolean" name="rv_centered"/>
        <attr format="enum" name="rv_type">
            <enum name="simpleRipple" value="0"/>
            <enum name="doubleRipple" value="1"/>
            <enum name="rectangle" value="2"/>
        </attr>
        <attr format="dimension" name="rv_ripplePadding"/>
        <attr format="boolean" name="rv_zoom"/>
        <attr format="float" name="rv_zoomScale"/>

    </declare-styleable>
</resources>